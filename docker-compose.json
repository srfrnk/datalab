{
    "version": "3.2",
    "services": {
        "datalab": {
            "image": "gcr.io/cloud-datalab/datalab:latest",
            "ports": [
                "127.0.0.1:8081:8080"
            ],
            "environment": [
                "PROJECT_ID=${PROJECT_ID}"
            ],
            "volumes": [
                {
                    "type":"volume",
                    "source": "./content",
                    "target": "/content",
                    "read_only": false
                }
            ]
        }
    }
}
